{
  "name": "desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "build": {
    "appId": "com.example.myapp",
    "productName": "MyApp",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.cjs",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npx electron .\"",
    "electron-build": "npm run build && electron .",
    "build-renderer": "vite build",
    "package": "npm run build-renderer && electron-builder --win"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.0",
    "wait-on": "^7.2.0"
  }
}
